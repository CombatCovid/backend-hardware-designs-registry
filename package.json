{
  "name": "fullstack-vue-graphql-starter",
  "version": "1.0.0",
  "description": "Starter for Full-Stack Vue-GraphQL-Apollo Projects",
  "main": "server.ts",
  "scripts": {
    "dev:node": "NODE_ENV=development nodemon --exec ts-node --files src/index.ts",
    "dev": "concurrently npm:dev:*",
    "gen:schema": "ts-node --files src/scripts/createTypes.ts",
    "format": "prettier --write 'src/**/*.ts'"
  },
  "keywords": [],
