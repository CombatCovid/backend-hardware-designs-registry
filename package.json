  "license": "ISC",
  "dependencies": {
    "apollo-server": "^2.9.7",
    "apollo-server-express": "^2.9.6",
    "bcrypt": "^4.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "glob": "^7.1.6",
    "graphql": "^14.5.8",
    "graphql-tools": "^4.0.5",
    "graphql-yoga": "^1.18.3",
    "http-status-codes": "^1.4.0",
    "js-logging-tool": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.2.1",
    "mongoose": "^5.9.6",
    "prettier": "^1.18.2",
    "ts-node": "^8.1.0",
    "typescript": "^3.5.3"
