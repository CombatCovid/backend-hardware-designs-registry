    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "concurrently": "^3.6.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "graphql-import": "0.7.1",
    "merge-graphql-schemas": "^1.7.6",
    "nodemon": "^2.0.3",
    "ts-node-dev": "^1.0.0-pre.44"
  }
